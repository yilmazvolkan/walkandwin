<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.boun.volkanyilmaz.walkandwin">

  <!--
     The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
     Google Maps Android API v2, but you must specify either coarse or fine
     location permissions for the 'MyLocation' functionality.
    -->
  <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
  <uses-permission android:name="android.permission.INTERNET" />

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name=".ChoseActivity"
      android:theme="@style/TamEkran">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
      android:name=".PasswordActivity"
      android:theme="@style/TamEkran">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".LoginActivity" />
    </activity>
    <activity
      android:name=".RegisterActivity"
      android:theme="@style/IndEkran">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".LoginActivity" />
    </activity>
    <activity
      android:name=".RegisterActivity2"
      android:theme="@style/ComEkran">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".LoginActivity2" />
    </activity>
    <activity
      android:name=".LoginActivity"
      android:theme="@style/IndEkran">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".ChoseActivity" />
    </activity>
    <activity
      android:name=".LoginActivity2"
      android:theme="@style/ComEkran">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".ChoseActivity" />
    </activity>
    <!--
     The API key for Google Maps-based APIs is defined as a string resource.
     (See the file "res/values/google_maps_api.xml").
     Note that the API key is linked to the encryption key used to sign the APK.
     You need a different API key for each encryption key, including the release key that is used to
     sign the APK for publishing.
     You can define the keys for the debug and release targets in src/debug/ and src/release/.
        -->
    <meta-data
      android:name="com.google.android.geo.API_KEY"
      android:value="@string/google_maps_key" />

    <activity
      android:name=".MapsActivity"
      android:label="@string/title_activity_maps"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".MapsActivity2"
      android:label="@string/title_activity_maps"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus1.ProfileActivity1"
      android:label="@string/title_activity_app"
      android:theme="@style/AppTheme.NoActionBar">
      <meta-data
        android:name="android.support.PARENT_ACTIVITY"
        android:value=".MapsActivity" />
    </activity>
    <activity
      android:name=".navigatorMenus2.ProfileActivity2"
      android:label="@string/title_activity_app"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus1.CampaignsActivity1"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus1.DistancesActivity1"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus1.HowWorksActivity1"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus2.MyMenuActivity2"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus2.CampaignsActivity2"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus2.HowWorksActivity2"
      android:theme="@style/AppTheme.NoActionBar" />
    <activity
      android:name=".navigatorMenus2.PointActivity2"
      android:theme="@style/AppTheme.NoActionBar" />

    <meta-data
      android:name="io.fabric.ApiKey"
      android:value="4a3d7aa0cf02c502734fd4557398bce83ac0b9a0" />

    <activity android:name=".MyMenuActivity1"></activity>
  </application>

</manifest>